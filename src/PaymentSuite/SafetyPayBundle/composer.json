{
    "name": "paymentsuite/safetypay-bundle",
    "type": "symfony-bundle",
    "description": "SafetyPay Bundle, part of Mmoreram Payment Suite",
    "keywords": ["safetypay", "payment", "ecommerce"],
    "license": "MIT",
    "authors": [
        {
            "name": "Santi Castells",
            "email": "santi.castells@fancybox.com"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "symfony/event-dispatcher": ">=2.2.3",
        "symfony/framework-bundle": ">=2.2.3",
        "symfony/form": ">=2.2.3",
        "symfony/config": ">=2.2.3",
        "symfony/http-kernel": ">=2.2.3",
        "symfony/dependency-injection": ">=2.2.3",
        "twig/twig": "~1.11",
        "paymentsuite/payment-core-bundle": "~1.1"
    },
    "target-dir": "PaymentSuite/SafetypayBundle",
    "autoload": {
        "psr-0": { "PaymentSuite\\SafetypayBundle": "" }
    },
    "minimum-stability": "stable",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
