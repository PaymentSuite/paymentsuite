{
    "name": "paymentsuite/google-wallet-bundle",
    "type": "symfony-bundle",
    "description": "Google Wallet Bundle, part of Payment Suite for Symfony 2",
    "keywords": ["google-wallet", "payment", "ecommerce"],
    "license": "MIT",
    "authors": [
        {
            "name": "Denys Pasishnyi",
            "email": "dpcat237@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "symfony/event-dispatcher": ">=2.2.3",
        "symfony/framework-bundle": ">=2.2.3",
        "symfony/form": ">=2.2.3",
        "symfony/config": ">=2.2.3",
        "symfony/http-kernel": ">=2.2.3",
        "symfony/dependency-injection": ">=2.2.3",
        "twig/twig": "~1.11",
        "paymentsuite/payment-core-bundle": ">=1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*"
    },
    "target-dir": "PaymentSuite/GoogleWalletBundle",
    "autoload": {
        "psr-0": { "PaymentSuite\\GoogleWalletBundle": "" }
    },
    "minimum-stability": "stable",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
