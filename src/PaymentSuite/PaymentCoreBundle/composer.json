{
    "name": "paymentsuite/payment-core-bundle",
    "type": "symfony-bundle",
    "description": "Payment Core Bundle, part and core of PaymentSuite Project",
    "keywords": ["payment", "ecommerce", "paymentsuite"],
    "license": "MIT",
    "authors": [
        {
            "name": "Marc Morera",
            "email": "yuhu@mmoreram.com"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "symfony/event-dispatcher": ">=2.2",
        "symfony/config": ">=2.2",
        "symfony/http-kernel": ">=2.2",
        "symfony/dependency-injection": ">=2.2",
        "psr/log": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*"
    },
    "target-dir": "PaymentSuite/PaymentCoreBundle",
    "autoload": {
        "psr-0": { "PaymentSuite\\PaymentCoreBundle": "" }
    },
    "minimum-stability": "stable",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
