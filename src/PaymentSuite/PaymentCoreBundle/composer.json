    "type": "symfony-bundle",
    "description": "PaymentCore PaymentSuite Component",
    "keywords": ["payment", "ecommerce", "paymentsuite"],
    "license": "MIT",
    "authors": [
        {
            "name": "Marc Morera",
            "email": "yuhu@mmoreram.com"
